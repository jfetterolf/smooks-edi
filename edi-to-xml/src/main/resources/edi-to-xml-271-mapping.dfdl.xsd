                    </xsd:element>
                    <xsd:element dfdl:initiator="EB" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="eligibility-benefit-3">
                        <xsd:complexType>
                            <xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat">
